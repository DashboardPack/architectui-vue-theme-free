{
  "name": "architectui-vue-free",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "serve": "vite",
    "lint": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@googlemaps/js-api-loader": "^2.0.2",
    "@popperjs/core": "^2.11.8",
    "@vue/compat": "^3.5.25",
    "animate-sass": "^0.8.2",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "^0.40.9",
    "chart.js": "^4.5.1",
    "core-js": "^3.47.0",
    "es6-promise": "^4.2.6",
    "leaflet": "^1.9.4",
    "pe7-icon": "^1.0.4",
    "perfect-scrollbar": "^1.4.0",
    "pinia": "^3.0.4",
    "regenerator-runtime": "^0.14.1",
    "vue": "^3.5.25",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3",
    "vue-sidebar-menu": "^5.9.0",
    "vue3-perfect-scrollbar": "^2.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vitest/ui": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.47",
    "prettier": "^3.7.4",
    "sass": "^1.95.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.8"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
  "lint-staged": {
    "*.{js,ts,vue}": ["prettier --write", "eslint --fix"],
    "*.{json,css,scss,md}": ["prettier --write"]
  }
}
